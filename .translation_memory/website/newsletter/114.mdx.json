    {
      "segment_id": "8d04259b",
      "source_content": "## Toward Docusaurus v2",
      "source_content_hash": "b00b32db5004edb55b8f70d24b64606b5011f5c07caea718f3a812cc8a3ff9a7",
      "node_type": "heading",
      "translatable": true,
